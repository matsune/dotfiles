[core]
  editor = vim

[user]
  email = yuma.matsune@gmail.com
  name = Yuma Matsune

[push]
  default = current

[alias]
  b = branch
  f = fetch
  fo = fetch origin
  fom = fetch origin main
  fod = fetch origin develop
  p = push
  puo = push -u origin
  puom = push -u origin main
  puod = push -u origin develop
  m = merge
  mom = merge origin/main
  mod = merge origin/develop
  c = checkout 
  cm = checkout main
  cb = checkout -b
  cd = checkout develop
  s = status

